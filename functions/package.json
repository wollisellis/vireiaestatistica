{
  "name": "bioestat-platform-functions",
  "version": "2.0.0",
  "description": "Cloud Functions para manter rankings pr√©-agregados automaticamente",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "deploy:ranking": "firebase deploy --only functions:updateClassRanking,functions:onUnifiedScoreUpdate,functions:regenerateRankings",
    "logs": "firebase functions:log",
    "test": "echo \"No tests configured\" && exit 0"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "private": true,
  "keywords": [
    "firebase",
    "cloud-functions",
    "ranking",
    "bioestat",
    "education"
  ],
  "author": "bioestat-platform",
  "license": "MIT"
}