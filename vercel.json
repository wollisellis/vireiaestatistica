{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "src/app/**/*.tsx": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/jogos"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
